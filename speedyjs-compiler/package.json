  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "cd cases && node ../index.js && llvm-dis fib.bc && s2wasm fib.s -o fib-opt.wast -l /Users/micha/.emscripten_cache/wasm/wasm_compiler_rt.a --emscripten-glue --global-base=1024 --initial-memory=16777216 && wasm-as fib-opt.wast -o fib-opt.wasm && cp fib-opt.wasm ../../../webassembly-benchmark/ && cd .."
  },
  "jest": {
