{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es6" ],
    "sourceMap": true
  },
  "include": [
    "cli.ts",
    "src/**/*"
  ]
}
