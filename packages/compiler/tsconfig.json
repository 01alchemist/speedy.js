{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es6", "es2016" ],
    "sourceMap": true
  },
  "include": [
    "index.ts",
    "src/**/*"
  ]
}
